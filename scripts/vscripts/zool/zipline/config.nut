	// Units per second on the zipline per 0.01s if health isn't set
	// default: 8

	::zipspeed <- 6;

// -------------------------------------------------------------------	

	// Height on the player on the zipline
	// default: 75

	::zipheight <- 75;

// -------------------------------------------------------------------	

	// Distance between each func_button "blob" (they're 48x48x48)
	// default: 100

	::zipblobdist <- 100;

// -------------------------------------------------------------------	

	// Are player able to press jump or crouch to detach?
	// default: true

	::zipcandrop <- true;

// -------------------------------------------------------------------	

	// Should lines be populated on game load?
	// default: false

	::zippopulateonload <- false;	

